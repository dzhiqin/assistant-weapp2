ec-canvas {
  width: 100%;
  height: 100%;
}
.data-bg {
  background: #fff;
}
.tab {
  display: flex;
}
.tab .tab-item {
  padding: 20rpx;
  background: #f4cccc;
  color: #333;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 50rpx;
}
.tab .active {
  color: #fff;
  background: linear-gradient(180deg,#f4cccc 50%,#ff9933)
}
